<ul class="styleguide-nav">
  <li class="styleguide-nav__title">Base<li>
  <%- @links.each do |nav_item| %>
    <li class="styleguide-nav__item">
      <a href=<%= "/styleguide/#{nav_item.downcase}" %> class="styleguide-nav__item-link"><%= nav_item %></a>
    </li>
  <% end %>
</ul>

<% @non_base_items.each do |title, elements| %>
  <ul class="styleguide-nav">
    <li class="styleguide-nav__title"><%= title %><li>
    <%- instance_variable_get("@#{elements}").each do |label, path| %>
      <li class="styleguide-nav__item">
        <a href=<%= "/styleguide/#{path}" %> class="styleguide-nav__item-link"><%= label %></a>
      </li>
    <% end %>
  </ul>
<% end %>
