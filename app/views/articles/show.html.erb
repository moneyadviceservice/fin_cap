<div class="l-constrained">
  <div class="row">
    <div class="hero" style="background-image: url(<%= article.hero_image_component %>);">
      <h1 class="hero__heading">
        <%= article.hero_description_component.html_safe %>
      </h1>
    </div>
  </div>
</div>

<div class="l-constrained">
  <div class="l-2col">
    <div class="l-2col-main">
      <h2><%= article.title %></h2>

      <%= article.body.html_safe %>
    </div>
    <div class="l-2col-side">
      <%= article.cta_links_component %>
      <%= article.download_component %>
      <%= article.feedback_component %>
    </div>
  </div>
 </div>
